<div>
  <h1 class="font-bold text-4xl">Monsters#index</h1>
  <p>Find me in app/views/monsters/index.html.erb</p>
</div>

<%= paginate @monsters %>

<div>
  <ul>
    <% @monsters.each do |monster| %>
      <li>
        <%= link_to monster['name'], monster_path(monster['name']) %>
      </li>
    <% end %>
  </ul>
</div>
